    <link rel="icon" href="favicon.ico">

		<title>Secure distributed cloud storage - airDrive</title>

    <!-- Bootstrap core CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    <!-- Custom styles for this template -->
    <link href="stylesheets/drive.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>
    <div class="dropzone"></div>

    <nav class="navbar navbar-fixed-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">airDrive</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <div class="navbar-form navbar-right">
            <a href="/"><button class="btn btn-primary">Sign out</button></a>
          </div>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container-fluid main-content">
      <div class="row">
        <div class="col-md-3 favorites">
          <ul class="list-group">
            <li class="list-group-item"><a href="#">Documents</a></li>
            <li class="list-group-item"><a href="#">Photos</a></li>
            <li class="list-group-item"><a href="#">Videos</a></li>
          </ul>
        </div>
        <div class="col-md-7 browser">
          <ol class="breadcrumb">
            <li><a href="#">airDrive</a></li>
            <li><a href="#">Products</a></li>
            <li class="active">Prototype</li>
          </ol>
          <div class="files">
            <div class="file folder">
              <a href="#">
                <span class="fa fa-4x fa-folder"></span>
                <span class="desc">Folder 1</span>
              </a>
            </div>
            <div class="file folder">
              <a href="#">
                <span class="fa fa-4x fa-folder"></span>
                <span class="desc">Folder 2</span>
              </a>
            </div>
            <div class="file folder">
              <a href="#">
                <span class="fa fa-4x fa-folder"></span>
                <span class="desc">Folder 3</span>
              </a>
            </div>
            <div class="file">
              <a href="#">
                <span class="fa fa-4x fa-file-text"></span>
                <span class="desc">Document 1</span>
              </a>
            </div>
            <div class="file">
              <a href="#">
                <span class="fa fa-4x fa-file-text"></span>
                <span class="desc">Document 2</span>
              </a>
            </div>
          </div>
        </div>
        <div class="col-md-2 links">
          <a href="#"><span class="fa fa-plus-circle"></span>Upload</a>
          <a href="#"><span class="fa fa-plus-circle"></span>New Folder</a>
          <div class="drag-option">
            <span class="fa fa-4x fa-fw fa-share-square-o"></span>
            Drag to share
          </div>
          <div class="drag-option">
            <span class="fa fa-4x fa-fw fa-trash"></span>
            Drag to trash
          </div>
        </div>
      </div>
    </div><!-- /.container -->

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    <script src="https://use.fontawesome.com/21153cb4c1.js"></script>
    <script src="/javascripts/jquery-ui.min.js"></script>
    <script src="/javascripts/drive.js"></script>
  </body>
</html>
