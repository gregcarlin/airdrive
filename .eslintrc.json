{
    "extends": "google",
    "installedESLint": true,
    "env": {
      "node": true,
      "browser": true,
      "jquery": true
    },
    "globals": {
      "_": true,
      "Resumable": true,
      "CryptoJS": true,
      "saveAs": true
    },
    "rules": {
        "curly": [2, "multi-line"],
        "no-floating-decimal": 2,
        "operator-linebreak": ["error", "before"],
        "no-alert": 1
    }
}
