    "debug": "2.2.0",
    "ejs": "2.4.2",
    "express": "4.14.0",
    "express-minify": "0.2.0",
    "forever": "0.15.2",
    "lodash": "4.13.1",
    "mongodb": "2.1.18",
    "nodemon": "1.9.2",
    "serve-favicon": "2.3.0",
    "storj": "1.4.0"
  },
  "devDependencies": {
    "eslint": "3.0.1",
